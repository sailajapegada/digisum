import java.util.Scanner;
public class Digisum{
 public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    System.out.print("Enter he number: ");
    int num=sc.nextInt();
    int count=0,sum=0;
    while (num !=0) {
        int a=num%10;
        sum +=a;
        num /=10;
    }
    System.out.println("Sum of digits"+sum);
}}
